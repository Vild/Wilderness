{
	"name": "wlang",
	"targetType": "none",
	"description": "The W language parser, compiler and runtime.",
	"copyright": "Copyright Â© 2015, Dan Printzell",
	"authors": ["Dan Printzell"],
	"dependencies": {
		"wlang:compiler": "*"
	},
	"subPackages": [
		{
			"name": "ast",
			"targetType": "library",
			"sourcePaths": ["ast/source"],
			"importPaths": ["ast/source"],
			"dependencies": {
				"deslog": "~>0.2.0"
			}
		},
		{
			"name": "compiler",
			"targetType": "executable",
