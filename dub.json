			"sourcePaths": ["compiler/source"],
			"importPaths": ["compiler/source"],
			"dependencies": {
				"wlang:ast": "*"
			}
		},
		{
			"name": "vm",
			"targetType": "executable",
			"targetName": "vw",
			"sourcePaths": ["vm/source"],
			"importPaths": ["vm/source"],
			"dependencies": {
				"wlang:ast": "*"
			}
		}
	]
